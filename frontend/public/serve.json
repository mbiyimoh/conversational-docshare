{
  "rewrites": [
    { "source": "/share/**", "destination": "/share.html" },
    { "source": "**", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "**/*.@(jpg|jpeg|gif|png|svg|ico)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
