{
  "name": "conversational-docshare-backend",
  "version": "1.0.0",
  "description": "Backend API for Conversational Document Sharing Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=../.env tsx watch --require dotenv/config src/index.ts",
    "prebuild": "prisma generate",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "db:cleanup": "DOTENV_CONFIG_PATH=../.env tsx --require dotenv/config scripts/cleanup.ts",
    "db:push": "DOTENV_CONFIG_PATH=../.env tsx --require dotenv/config scripts/db-push.ts"
  },
  "keywords": [
    "api",
    "express",
    "typescript",
    "document-sharing"
  ],
  "author": "",
  "license": "MIT",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "jose": "^5.9.6",
    "mammoth": "^1.8.0",
    "multer": "^1.4.5-lts.1",
    "next-auth": "^4.24.13",
    "openai": "^4.72.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.0.379",
    "pg": "^8.13.1",
    "pgvector": "^0.2.0",
    "uuid": "^13.0.0",
    "workerpool": "^10.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "axios": "^1.13.2",
    "eslint": "^9.15.0",
    "jest": "^30.2.0",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
